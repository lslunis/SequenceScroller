{
  "manifest_version": 2,
  "name": "Sequence Scroller",
  "version": "1.0.1",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ]
}
